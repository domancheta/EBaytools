<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="org.allthegoodstuff.ebaytools.view.SalesItemsViewController"
            prefHeight="400.0" prefWidth="1024.0">
    <TabPane tabClosingPolicy="UNAVAILABLE">
        <Tab text="Watch">
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="700.0" prefWidth="1024.0">
                <TableView fx:id="saleItemTable" layoutX="70.0" layoutY="36.0" prefHeight="700.0" prefWidth="1024.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                    <columns>
                        <TableColumn fx:id="itemIDColumn" text="Item #" />
                        <TableColumn fx:id="titleColumn" prefWidth="75.0" text="Item Title" />
                        <TableColumn fx:id="priceColumn" text="Price" />
                        <TableColumn fx:id="startTimeColumn" prefWidth="75.0" text="Start Time" />
                        <TableColumn fx:id="endTimeColumn" prefWidth="75.0" text="End Time" />
                        <TableColumn fx:id="sellerColumn" prefWidth="75.0" text="Seller" />
                        <TableColumn prefWidth="75.0" text="Image" />
                    </columns>
                    <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                    </columnResizePolicy>
                </TableView>
            </AnchorPane>
        </Tab>
        <Tab text="Search">
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
        </Tab>
        <Tab text="Database">
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
        </Tab>
        <Tab text="Misc">
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
        </Tab>
    </TabPane>
</AnchorPane>
